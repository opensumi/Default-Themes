{
  "name": "ide-theme-defaults",
  "displayName": "%displayName%",
  "description": "%description%",
  "categories": [
    "Themes"
  ],
  "version": "1.3.0",
  "publisher": "cloud-ide",
  "engines": {
    "vscode": "*"
  },
  "scripts": {
    "version": "standard-version",
    "prepack": "tnpm i && npm run version && rm -rf node_modules && tnpm i",
    "pack": "node pack.js"
  },
  "contributes": {
    "themes": [
      {
        "id": "cloudide-dark",
        "label": "%theme.name.dark%",
        "uiTheme": "vs-dark",
        "path": "./themes/dark_plus.json"
      }
    ]
  },
  "devDependencies": {
    "@alipay/cloud-ide-ext-vscode-extension-builder": "^3.0.6",
    "runscript": "^1.4.0",
    "standard-version": "^7.0.0"
  }
}
