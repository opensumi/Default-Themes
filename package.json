{
  "name": "opensumi-default-themes",
  "displayName": "OpenSumi Default Themes",
  "description": "The default OpenSumi themes",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "opensumi",
    "theme",
    "themes"
  ],
  "version": "2.5.0",
  "publisher": "opensumi",
  "engines": {
    "vscode": "*"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:opensumi/Default-Themes.git"
  },
  "publishConfig": {
    "registry": "http://registry.npm.alibaba-inc.com"
  },
  "icon": "icons/icon.png",
  "contributes": {
    "themes": [
      {
        "id": "opensumi-dark",
        "label": "OpenSumi Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/dark/plus.json"
      },
      {
        "id": "opensumi-light",
        "label": "OpenSumi Light",
        "uiTheme": "vs",
        "path": "./themes/light/plus.json"
      }
    ]
  }
}
